{
  "name": "petproject",
  "version": "1.0.0",
  "description": "MERN(project)",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "babel src --out-file server.js  --presets es2015",
    "watch": "babel src --out-file server.js  --presets es2015 -w",
    "sinc": "concurrently \"npm run watch\" \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "engines": {
    "node": "10.13.0"
  },
  "author": "Evgenii Shevchenko",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bson": "^4.0.2",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "mongodb-stitch-browser-sdk": "^4.4.0",
    "mongodb-stitch-server-sdk": "^4.4.0",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.1.0"
  }
}
